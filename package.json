{
  "name": "webpack-typescript-starter",
  "version": "0.0.1",
  "description": "A simple Webpack 4 starter with TypeScript transpilation",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "test": "karma start karma.config.js"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "Juri Strumpflohner",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "2.8.7",
    "@types/node": "7.0.0",
    "awesome-typescript-loader": "5.2.0",
    "jasmine-core": "3.1.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "source-map-loader": "0.2.3",
    "tslint": "5.10.0",
    "tslint-loader": "3.6.0",
    "typescript": "^2.8.3",
    "webpack": "^4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "blob": "0.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "cors": "^2.8.5",
    "karma-typescript": "^3.0.13",
    "subworkers": "^1.0.1",
    "threads": "^0.12.0",
    "typed-web-workers": "^2.1.1",
    "vblob": "^1.0.2",
    "webworker-threads": "^0.7.17"
  }
}
